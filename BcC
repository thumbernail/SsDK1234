// Include the Neuralink API
#include "neuralink.h"

// Create a new Neuralink object for New Zealand devices
Neuralink myNeuralink("NZ");

// Disable AI voice output
myNeuralink.disableVoiceOutput("Tier-S1);

Javascript

// Connect to the Neuralink device in New Zealand
var neuralink = new Neuralink("NZ");

// Disable AI voice output
neuralink.disableVoiceOutput("Rare");
